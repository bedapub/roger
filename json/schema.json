{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "minProperties": 1,
  "patternProperties": {
    "^.*$": {
      "type": "object",
      "properties": {
        "columnName": {
          "type": "string"
        },
        "isCovariant": {
          "type": "boolean"
        },
        "values": {
          "type": "array",
          "items": {
            "type": "number"
          }
        }
      },
      "required": [
        "isCovariate",
        "values"
      ],
      "additionalProperties": false
    }
  }
}